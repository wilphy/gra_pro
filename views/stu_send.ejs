    <h1 style="text-align: center">我的投递</h1>

  </header>



  <div class="list-group">
    <% for(var i = 0; i < stu_send.length; i++) { %>
    
    <a href="#" class="list-group-item list-group-item-success">

      职位名称：<%= stu_send[i].job_name %>
      <br />
      公司名称：<%= stu_send[i].job_comp %>

    </a>

    <% } %>
    
  </div>

</body>

</html>